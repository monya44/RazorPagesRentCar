@model IEnumerable<DistHeadCount> 


<table class="table table-hover rounded-pill">
    <thead class="thead-light">
        <tr>
            <th>District</th>
            <th>Head Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.District</td>
                <td>@item.Count</td>
            </tr>
        }
    </tbody>
</table>